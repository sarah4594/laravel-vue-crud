{
    "private": true,
    "scripts": {
        "build": "npm run css:build",
        "dev": "npm run development",
        "development": "mix",
        "watch": "concurrently \"mix watch\" \"npm run css:watch\"",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "css:build": "tailwindcss -i resources/css/tailwind.css -o public/css/tailwind.css",
        "css:watch": "tailwindcss -i resources/css/tailwind.css -o public/css/tailwind.css --watch"
    },
    "devDependencies": {
        "@popperjs/core": "^2.10.2",
        "autoprefixer": "^10.4.2",
        "axios": "^0.25",
        "bootstrap": "^5.1.3",
        "concurrently": "^7.0.0",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.4.6",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.32.11",
        "sass-loader": "^11.0.1",
        "tailwindcss": "^3.0.22",
        "tailwindcss-cli": "^0.1.2",
        "vue": "^2.6.14",
        "vue-loader": "^15.9.8",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "vee-validate": "^2.2.15",
        "vue-axios": "^3.4.1",
        "vue-router": "^3.5.3",
        "vuex": "^3.6.2"
    }
}
